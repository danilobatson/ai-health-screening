{
  "version": 2,
  "builds": [
    {
      "src": "api/assess-health.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/health.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/assess-health",
      "dest": "/api/assess-health.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": false,
    "autoJobCancelation": true,
    "silent": false
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": false
    }
  },
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@3.9.16"
    }
  }
}
