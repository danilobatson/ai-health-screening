{
  "version": 2,
  "functions": {
    "api/*.py": {
      "runtime": "@vercel/python@3.9.16"
    }
  },
  "routes": [
    {
      "src": "/api/assess-health",
      "dest": "/api/assess-health.py"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "github": {
    "enabled": true,
    "autoAlias": true,
    "autoJobCancelation": true,
    "silent": false,
    "deployOnPush": true,
    "ignoreBuildStepFailure": false
  },
  "git": {
    "deploymentEnabled": {
      "main": true,
      "develop": false
    }
  }
}
